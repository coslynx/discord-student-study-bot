{
  "name": "discord-student-study-bot",
  "version": "1.0.0",
  "description": "A Discord bot designed to help students find and share study materials, collaborate in study groups, and access personalized learning resources.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest",
    "build": "tsc && tsc-watch --onSuccess \"npm run build:prod\"",
    "build:prod": "tsc && tsc-watch --onSuccess \"npm run build:prod\"",
    "deploy": "npm run build && docker build -t discord-student-study-bot . && docker push discord-student-study-bot && heroku container:push web -i discord-student-study-bot && heroku container:release web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coslynx/discord-student-study-bot.git"
  },
  "author": "Coslynx",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cloudinary": "^1.38.0",
    "discord.js": "^14.8.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^87.0.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^2.3.3",
    "node-fetch": "^3.3.0",
    "openai": "^3.4.0",
    "prisma": "^4.12.1",
    "tailwindcss": "^3.3.2",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cloudinary": "^1.37.1",
    "@types/discord.js": "^14.8.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mysql2": "^2.3.3",
    "@types/node": "^18.16.9",
    "@types/node-fetch": "^3.0.0",
    "@types/openai": "^3.4.1",
    "@types/prisma": "^4.12.1",
    "@types/tailwindcss": "^3.2.4",
    "@types/workbox-webpack-plugin": "^6.5.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "jest": "^29.6.4",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.22",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.4.1"
  }
}